{
  "name": "@motioneffector/wiki",
  "version": "0.1.0",
  "description": "A TypeScript library for creating and managing interconnected knowledge bases with bidirectional linking",
  "author": "MotionEffector",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/motioneffector/wiki"
  },
  "homepage": "https://github.com/motioneffector/wiki#readme",
  "bugs": {
    "url": "https://github.com/motioneffector/wiki/issues"
  },
  "keywords": [
    "wiki",
    "knowledge-base",
    "bidirectional-links",
    "backlinks",
    "note-taking",
    "zettelkasten",
    "worldbuilding",
    "graph",
    "linking",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run test:run && npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8"
  }
}
